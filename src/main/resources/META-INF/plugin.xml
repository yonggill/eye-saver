<idea-plugin>
    <id>com.github.yonggill.eyesaver</id>
    <name>eye-saver</name>
    <vendor>lupin-the-cat</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="com.github.yonggill.eyesaver" path="/theme/eye-saver.theme.json"/>
    </extensions>

</idea-plugin>
